<h1><%= t('.title') %></h1>

<%= form_with model: @form, url: items_path, local: true do |form| %>
  <%= form.fields_for :items do |f| %>
    <%= render 'shared/error_messages', object: f.object %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :degree %>
    <%= f.text_field :degree %>
    <%= f.label :start_date %>
    <%= f.text_field :start_date %>
    <%= f.label :finish_date %>
    <%= f.text_field :finish_date %>
    <%= f.label :price %>
    <%= f.text_field :price %>
  <% end %>
  <%= form.submit %>
<% end %>
