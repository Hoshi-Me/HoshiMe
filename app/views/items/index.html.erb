<h1>一覧</h1>
<% @items.each do |item| %>
  <p>欲しいもの</p>
  <span><%= item.name %></span>
  <%= link_to edit_item_path(item) do %>
    <i class="fas fa-pen"></i>
  <% end %>
  <%= link_to item_path(item), method: :delete, data: {confirm: "削除してもいいですか？"} do %>
    <i class="fas fa-trash"></i>
  <% end %>
<% end %>
